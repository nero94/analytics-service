{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "object-curly-spacing": "off",
    "max-len": "off",
    "new-cap": "off"
  }
}